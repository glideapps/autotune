{
    "name": "autotune",
    "license": "Apache-2.0",
    "version": "2.0.0",
    "description": "Automatically tune JavaScript apps",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "unpkg": "dist/index.browser.min.js",
    "scripts": {
        "build": "script/build.sh",
        "bump-version": "ts-node script/patch-npm-version.ts",
        "watch": "watch --filter=script/watch-filter.js 'npm run build' src",
        "serve": "serve -l 3001",
        "start": "concurrently 'npm run watch' 'npm run serve' 'sleep 1 && open http://localhost:3001/samples'",
        "tslint": "tslint --project .",
        "test": "jest"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/quicktype/autotune.git"
    },
    "author": "typeguard, Inc.",
    "bin": {
        "tune": "dist/cli/index.js",
        "autotune": "dist/cli/index.js"
    },
    "bugs": {
        "url": "https://github.com/quicktype/autotune/issues"
    },
    "homepage": "https://github.com/quicktype/autotune#readme",
    "dependencies": {
        "aws-sdk": "^2.247.1",
        "chalk": "^2.4.1",
        "jsonwebtoken": "^8.2.2",
        "moment": "^2.22.2",
        "node-persist": "^3.0.1",
        "request": "^2.87.0",
        "request-promise": "^4.2.2",
        "table": "^4.0.3",
        "yargs": "^11.0.0"
    },
    "devDependencies": {
        "@types/jest": "^23.1.1",
        "@types/jsonwebtoken": "^7.2.7",
        "@types/node": "^8.10.12",
        "@types/node-persist": "0.0.33",
        "@types/request-promise": "^4.1.41",
        "@types/shelljs": "^0.8.0",
        "@types/table": "^4.0.5",
        "@types/yargs": "^11.0.0",
        "browserify": "^16.2.2",
        "compare-versions": "^3.2.1",
        "concurrently": "^3.5.1",
        "jest": "^23.1.0",
        "quicktype": "^15.0.7",
        "serve": "^8.1.3",
        "shelljs": "^0.8.2",
        "ts-jest": "^22.4.6",
        "ts-node": "^6.0.5",
        "tslint": "^5.10.0",
        "typescript": "^2.8.3",
        "uglify-js": "^3.3.27",
        "watch": "^1.0.2"
    }
}
